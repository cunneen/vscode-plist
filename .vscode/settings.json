{
  "files.exclude": {
    "out": true,
    "dist": true,
    "node_modules": true,
    ".vscode-test": true,
    "yarn.lock": true,
    "ui/webview.js*": true
  },
  "search.exclude": {
    "package-lock.json": true,
    "**/node_modules": true,
    "**/*.code-search": true,
    "*.js": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
